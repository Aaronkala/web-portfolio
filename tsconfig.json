{
  "compileOnSave": true,
  "compilerOptions": {
    "baseUrl": "./",
    "allowSyntheticDefaultImports": true,
    "downlevelIteration": true,
    "strictNullChecks": true,
    "skipLibCheck": true,
    "typeRoots": ["./node_modules/@types"],
    "lib": ["esnext", "dom"],
    "rootDir": "src",
    "jsx": "react",
    "noEmit": true,
    "allowJs": true,
    "checkJs": true,
    "esModuleInterop": true
  },
  "include": [
    "src",
    "static",
    "config",
    ".storybook",
    "gatsby-browser.js",
    "gatsby-config.js",
    "gatsby-node.js",
    "gatsby-ssr.js"
  ]
}
